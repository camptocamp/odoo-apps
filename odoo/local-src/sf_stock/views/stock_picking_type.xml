<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_sf_picking_type_form" model="ir.ui.view">
            <field name="name">view.sf.picking.type.form</field>
            <field name="model">stock.picking.type</field>
            <field name="inherit_id" ref="stock.view_picking_type_form"/>
            <field name="priority" eval="100"/>
            <field name="arch" type="xml">
                <field name="use_existing_lots" position="after">
                    <field name="show_button_shipped"/>
                    <field name="propagate_delivery_info"/>
                    <field name="show_availability_wizard"/>
                    <field name="allow_unassign_lot"/>
                </field>
            </field>
    </record>
</odoo>