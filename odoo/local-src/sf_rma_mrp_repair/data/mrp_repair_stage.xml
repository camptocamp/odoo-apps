<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="stage_draft" model="mrp.repair.stage">
        <field name="name">draft</field>
        <field name="sequence">10</field>
        <field name="fold">True</field>
    </record>
    <record id="stage_open" model="mrp.repair.stage">
        <field name="name">open</field>
        <field name="sequence">20</field>
        <field name="fold">False</field>
    </record>
    <record id="stage_to_analyze" model="mrp.repair.stage">
        <field name="name">to_analyze</field>
        <field name="sequence">30</field>
        <field name="fold">False</field>
    </record>
    <record id="stage_to_quotation" model="mrp.repair.stage">
        <field name="name">to_quotation</field>
        <field name="sequence">40</field>
        <field name="fold">False</field>
    </record>
    <record id="stage_under_repair" model="mrp.repair.stage">
        <field name="name">under_repair</field>
        <field name="sequence">50</field>
        <field name="fold">False</field>
    </record>
    <record id="stage_to_test" model="mrp.repair.stage">
        <field name="name">to_test</field>
        <field name="sequence">60</field>
        <field name="fold">False</field>
    </record>
    <record id="stage_to_finalize" model="mrp.repair.stage">
        <field name="name">to_finalize</field>
        <field name="sequence">70</field>
        <field name="fold">False</field>
    </record>
    <record id="stage_done" model="mrp.repair.stage">
        <field name="name">done</field>
        <field name="sequence">80</field>
        <field name="fold">True</field>
    </record>
    <record id="stage_cancel" model="mrp.repair.stage">
        <field name="name">cancel</field>
        <field name="sequence">90</field>
        <field name="fold">True</field>
    </record>
</odoo>
