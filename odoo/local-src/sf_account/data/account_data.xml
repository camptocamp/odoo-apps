<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Down payment invoice sequence-->
        <record id="downpay_invoice_seq" model="ir.sequence">
            <field name="name">Downpayment Invoice sequence</field>
            <field name="prefix">A%(range_year)s%(month)s%(day)s</field>
            <field name="code">downpay.invoice</field>
            <field name="number_next" eval="1"/>
            <field name="number_increment" eval="1"/>
            <field name="padding" eval="4"/>
            <field name="company_id" eval="False" />
        </record>
    </data>
</odoo>
